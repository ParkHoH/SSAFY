<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean name="col" class="com.collection.MyCollectionImpl">
	<property name="array" >
		<array>
			<value>100</value>
			<value>200</value>
			<value>300</value>
		</array>
	</property>
	<property name="list">
		<list>
			<value>red</value>
			<value>blue</value>
			<value>green</value>
			<value>white</value>
		</list>
	</property>
	<property name="set">
		<set>
			<value>Kim</value>
			<value>James</value>
			<value>Park</value>
		</set>
	</property>
	<property name="map">
		<map>
			<entry>
				<key>
					<value>num</value>
				</key>
				<value>123</value>
			</entry>
			<entry>
				<key>
					<value>name</value>
				</key>
				<value>Lee June</value>
			</entry>
			<entry>
				<key>
					<value>address</value>
				</key>
				<value>london</value>
			</entry>
		</map>
	</property>
		
</bean>

</beans>
